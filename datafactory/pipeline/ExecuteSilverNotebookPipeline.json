{
	"name": "ExecuteSilverNotebookPipeline",
	"properties": {
		"activities": [
			{
				"name": "ForEach ExecutionNotebooks",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@split(pipeline().parameters.execution_notebooks, ',')",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Execute Notebook",
							"type": "DatabricksNotebook",
							"state": "Inactive",
							"onInactiveMarkAs": "Succeeded",
							"dependsOn": [],
							"policy": {
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"notebookPath": "/SilverNotebooks/GenericTransformationNotebook",
								"baseParameters": {
									"business_unit": {
										"value": "@pipeline().parameters.business_unit",
										"type": "Expression"
									},
									"source_path": {
										"value": "@pipeline().parameters.source_path",
										"type": "Expression"
									},
									"destination_path": {
										"value": "@pipeline().parameters.destination_path",
										"type": "Expression"
									}
								}
							},
							"linkedServiceName": {
								"referenceName": "AzureDatabricksDatapoc",
								"type": "LinkedServiceReference"
							}
						},
						{
							"name": "Execute Notebook_copy1",
							"type": "DatabricksNotebook",
							"dependsOn": [],
							"policy": {
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"notebookPath": {
									"value": "@concat('/SilverNotebooks/', item())",
									"type": "Expression"
								},
								"baseParameters": {
									"business_unit": {
										"value": "@pipeline().parameters.business_unit",
										"type": "Expression"
									},
									"source_path": {
										"value": "@pipeline().parameters.source_path",
										"type": "Expression"
									},
									"destination_path": {
										"value": "@pipeline().parameters.destination_path",
										"type": "Expression"
									}
								}
							},
							"linkedServiceName": {
								"referenceName": "AzureDatabricksDatapoc",
								"type": "LinkedServiceReference"
							}
						}
					]
				}
			}
		],
		"parameters": {
			"business_unit": {
				"type": "string"
			},
			"source_path": {
				"type": "string"
			},
			"destination_path": {
				"type": "string"
			},
			"execution_notebooks": {
				"type": "string"
			}
		},
		"folder": {
			"name": "2-Silver"
		},
		"annotations": []
	}
}